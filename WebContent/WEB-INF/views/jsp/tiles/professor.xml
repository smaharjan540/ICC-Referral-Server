<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC
       "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN"
       "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>
	<definition name="professoradmin" template="/WEB-INF/views/jsp/layouts/main.jsp">
		<put-attribute name="title" value="Professor page" />
		<put-attribute name="header" type="template"
			value="/WEB-INF/views/jsp/templates/header.jsp" />
		<put-attribute name="leftmenu" type="template"
			value="/WEB-INF/views/jsp/templates/leftmenu.jsp" />
		<put-attribute name="navigation" type="template"
			value="/WEB-INF/views/jsp/templates/navigation.jsp" />
		<put-attribute name="content" type="template"
			value="/WEB-INF/views/jsp/templates/container.jsp" />
		<put-attribute name="footer" type="template"
			value="/WEB-INF/views/jsp/templates/footer.jsp" />
	</definition>
	<!-- Student Page -->
	<definition name="student" extends="professoradmin">
		<put-attribute name="title" value="Student page" />
		<put-attribute name="content" type="template"
			value="/WEB-INF/views/jsp/templates/professor/student.jsp" />
	</definition>
	<!-- Resourse Page -->
	<definition name="resourceprofessor" extends="professoradmin">
		<put-attribute name="title" value="Resourse page" />
		<put-attribute name="content" type="template"
			value="/WEB-INF/views/jsp/templates/professor/resource.jsp" />
	</definition>
	<!-- Discussion Page -->
	<definition name="discussion" extends="professoradmin">
		<put-attribute name="title" value="Discussion page" />
		<put-attribute name="content" type="template"
			value="/WEB-INF/views/jsp/templates/professor/discussion.jsp" />
	</definition>
	<!-- Answer Page -->
	<definition name="answer" extends="professoradmin">
		<put-attribute name="title" value="Answer page" />
		<put-attribute name="content" type="template"
			value="/WEB-INF/views/jsp/templates/professor/answer.jsp" />
	</definition>
	<!-- Message Page -->
	<definition name="message" extends="professoradmin">
		<put-attribute name="title" value="Message page" />
		<put-attribute name="content" type="template"
			value="/WEB-INF/views/jsp/templates/professor/message.jsp" />
	</definition>

</tiles-definitions>